{
  "rules": {
    "tours": {
      ".read": true,
      ".write": true,
      ".indexOn": ["status", "createdAt", "price"]
    },
    "blogs": {
      ".read": true,
      ".write": true,
      ".indexOn": ["status", "createdAt", "category"]
    },
    "bookings": {
      ".read": true,
      ".write": true,
      ".indexOn": ["status", "createdAt", "userId", "tourId"]
    },
    "settings": {
      ".read": true,
      ".write": true,
      ".indexOn": ["updatedAt"]
    },
    "notifications": {
      ".read": true,
      ".write": true,
      ".indexOn": ["userId", "createdAt", "read"]
    },
    "users": {
      ".read": true,
      ".write": true,
      ".indexOn": ["email", "createdAt", "isAdmin"]
    },
    "popupAds": {
      ".read": true,
      ".write": true,
      ".indexOn": ["status", "createdAt"]
    },
    "emailNotifications": {
      ".read": true,
      ".write": true,
      ".indexOn": ["createdAt", "type"]
    },
    "test": {
      ".read": true,
      ".write": true
    },
    "diagnostics": {
      ".read": true,
      ".write": true
    },
    "$other": {
      ".read": true,
      ".write": true
    }
  }
}